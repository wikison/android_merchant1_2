<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:id="@+id/ll_root"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context="com.zemult.merchant.activity.ShareAppointmentActivity">


    <WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/rl_share"
        android:layout_below="@+id/head"/>

    <LinearLayout
        android:id="@+id/ll_back"
        android:layout_width="50dp"
        android:layout_height="48dp"
        android:clickable="true"
        android:gravity="center_vertical">

        <Button

            android:id="@+id/lh_btn_back"
            android:layout_width="@dimen/j44dp"
            android:layout_height="@dimen/j44dp"
            android:background="@mipmap/back_btn"
            android:gravity="center"/>

    </LinearLayout>


</FrameLayout>
